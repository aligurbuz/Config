{
    "name": "obullo/config",
    "type": "library",
    "description": "Configuration file loader built on zend/config package that comes with environment support.",
    "keywords": [
        "config",
        "reader",
        "zend",
        "environment"
    ],
    "homepage": "http://config.obullo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ersin Guvenc",
            "email": "eguvenc@gmail.com",
            "homepage": "http://obullo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Obullo\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "": ""
        }
    },
    "require": {
        "php": "^7.0",
        "zendframework/zend-config": "^3.1",
        "symfony/yaml": "^3.0",
        "zendframework/zend-filter": "^2.8",
        "zendframework/zend-config-aggregator": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "zendframework/zend-servicemanager": "^3.3"
    }
}
