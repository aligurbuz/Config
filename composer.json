{
    "name": "obullo/config",
    "type": "library",
    "description": "Obullo config is a package that assumes configuration management by reading the configuration files in your application.",
    "keywords": [
        "config",
        "reader",
        "zend"
    ],
    "homepage": "http://config.obullo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ersin Guvenc",
            "email": "eguvenc@gmail.com",
            "homepage": "http://obullo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Obullo\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "": ""
        }
    },
    "require": {
        "php": "^7.0",
        "zendframework/zend-config": "^3.1",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
